const students = [];

function capitalizeName(raw) {
  return raw
    .trim()
    .split(/\s+/)
    .map((w) => (w[0] || "").toUpperCase() + (w.slice(1) || "").toLowerCase())
    .join(" ");
}

for (let i = 0; i < numStudent; i++) {
  // name
